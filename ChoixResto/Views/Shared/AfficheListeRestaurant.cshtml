
@model List<ChoixResto.Models.Restaurant> 
<!-- vue partielle modèle injecté -->
<!-- Liste obtenu depuis le contrôleur -->
<table>
    <tr>
        <th>Nom</th>
        <th>Téléphone</th>
    </tr>
    @foreach (var resto in Model)
    {
        <tr>
            <td>@resto.Nom</td>
            <td>@resto.Telephone</td>
        </tr>
    }
</table>